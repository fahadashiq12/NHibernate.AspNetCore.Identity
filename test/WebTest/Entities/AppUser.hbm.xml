<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="urn:nhibernate-mapping-2.2"
  namespace="WebTest.Entities" assembly="WebTest">

  <joined-subclass name="AppUser" table="app_users" schema="public" extends="NHibernate.AspNetCore.Identity.IdentityUser, NHibernate.AspNetCore.Identity, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
    <key column="id" />
    <property name="CreateTime" column="create_time" type="datetime" not-null="true" generated="insert" update="false" insert="false" />
    <property name="LastLogin" column="last_login" type="datetime" not-null="false" />
    <property name="LoginCount" column="login_count" type="int" not-null="true" />
    <!-- <many-to-one name="City" column="city_id" not-found="ignore" /> -->
  </joined-subclass>

</hibernate-mapping>

